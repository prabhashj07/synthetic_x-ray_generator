// Jest Snapshot v1, https://goo.gl/fbAQLP

exports[`circle slider should render a default circle slider 1`] = `
<svg
  height="180px"
  onMouseDown={[Function]}
  onTouchStart={[Function]}
  style={
    Object {
      "boxSizing": "border-box",
      "padding": "5px",
    }
  }
  viewBox="0 0 180 180"
  width="180px"
>
  <g>
    <circle
      cx={90}
      cy={90}
      r={70}
      style={
        Object {
          "fill": "none",
          "stroke": "#e9eaee",
          "strokeWidth": 5,
        }
      }
    />
    <path
      d="M90 160 A 70 70 0 0 1 90.0007 159.99999999649998"
      style={
        Object {
          "fill": "none",
          "stroke": "#007aff",
          "strokeLinecap": "round",
          "strokeWidth": 20,
        }
      }
    />
    <filter
      filterUnits="userSpaceOnUse"
      id="dropShadow"
    >
      <feGaussianBlur
        in="SourceAlpha"
        stdDeviation="3"
      />
      <feOffset
        dx="2"
        dy="2"
      />
      <feComponentTransfer>
        <feFuncA
          slope="0.3"
          type="linear"
        />
      </feComponentTransfer>
      <feMerge>
        <feMergeNode />
        <feMergeNode
          in="SourceGraphic"
        />
      </feMerge>
    </filter>
    <circle
      cx={90.0007}
      cy={159.99999999649998}
      filter="url(#dropShadow)"
      r={20}
      style={
        Object {
          "cursor": "pointer",
          "fill": "#fff",
        }
      }
    />
  </g>
</svg>
`;
